{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "love",
    "plugin:react/jsx-runtime",
    "eslint-config-prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "@typescript-eslint/semi": 0,
    "@typescript-eslint/comma-dangle": 0
  },
  "ignorePatterns": ["vite.config.ts", "update-segmenter-files.js"]
}
